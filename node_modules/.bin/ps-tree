#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/ps-tree/bin/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/ps-tree/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/node_modules:/mnt/c/Users/Ramtin/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/ps-tree/bin/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/ps-tree/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/node_modules:/mnt/c/Users/Ramtin/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules:/mnt/c/Users/Ramtin/PycharmProjects/MERN_to_do_app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ps-tree/bin/ps-tree.js" "$@"
else
  exec node  "$basedir/../ps-tree/bin/ps-tree.js" "$@"
fi
